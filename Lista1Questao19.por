programa
{
    funcao inicio()
    {
        inteiro num1, num2

        escreva("Digite o primeiro número: ")
        leia(num1)
        escreva("Digite o segundo número: ")
        leia(num2)

        se (num1 > 0 e num2> 0) 
            escreva("A soma dos dois números é: ", num1 + num2)
        senao
            escreva("Ambos os números devem ser positivos.")
        
    }
}
