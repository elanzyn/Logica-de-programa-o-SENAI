programa
{
   funcao inicio()
   {
      inteiro numero1, numero2, soma
      escreva("Digite o primeiro número inteiro: ")
      leia(numero1)

      escreva("Digite o segundo número inteiro: ")
      leia(numero2)

      soma = numero1 + numero2
      escreva("A soma entre ", numero1, " e ", numero2, " é: ", soma, "\n")
   }
}
