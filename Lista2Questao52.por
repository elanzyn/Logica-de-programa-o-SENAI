programa
{
   funcao inicio()
   {
      real A, B, C, delta
      escreva("Digite o valor de A: ")
      leia(A)

      escreva("Digite o valor de B: ")
      leia(B)

      escreva("Digite o valor de C: ")
      leia(C)

      delta = (B * B) - (4 * A * C)
      escreva("O valor de Delta (Δ) é: ", delta, "\n")
   }
}
