programa
{
   funcao inicio()
   {
      real numero, dobro, terco
      escreva("Digite um número real: ")
      leia(numero)

      dobro = numero * 2
      terco = numero / 3
      escreva("O dobro de ", numero, " é: ", dobro, "\n")
      escreva("A terça parte de ", numero, " é: ", terco, "\n")
   }
}
