programa {
  funcao inicio() {
    inteiro num1 , num2
    escreva(" Insira um número: ")
    leia(num1)

    escreva(" Insira um número: ")
    leia(num2)

    se(num1 % 2 == 0 e num2 % 2 == 0)
    {
      escreva(" Ambos  os números são pares. ")
    }
    senao
    {
      escreva(" Um dos números não é par. ")
    }
  }
}
